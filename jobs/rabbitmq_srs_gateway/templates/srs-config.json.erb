{
  "backbone_amqpurl": "amqp://<%= properties.rabbitmq_srs.backbone_user %>:<%= properties.rabbitmq_srs.backbone_password %>@<%= properties.rabbitmq_srs.backbone_address %>:<%= properties.rabbitmq_srs.backbone_port %>/",
  "cloud": "vcap",
  "code_dir": "/var/vcap/packages/rabbitmq_srs_builds",
  "default_build": "rabbitmq_v2_4_1",
  "public_instance_port_range": [10000, 40000],
  "private_instance_port_range": [40000, 50000],
  "vcap_service_offering": {
    "name": "rabbitmq",
    "version": "2.4",
    "plans": ["free"],
    "tags": ["rabbitmq"],
    "acls": <%= properties.rabbitmq_srs.acls %>,
    "token": "<%= properties.rabbitmq_srs.token %>"
  },
  "vcap_cloudcontroller_base_url": "<%= properties.cc.srv_api_uri %>"
}
